?# define
?|Incr(?var, ?incr)

?# augassign
?var += ?incr

?# var_first
?var = ?var + ?incr

?# incr_first
?var = ?incr + ?var


?# define
?|Comp(?left, ?right)

?# eq
?left == ?right

?# ne
?left != ?right

?# lt
?left < ?right

?# le
?left <= ?right

?# gt
?left > ?right

?# ge
?left >= ?right


?# define
?|Loop(?i, ?n)

?# for_loop
for ?i in ?range(?n):
    ?

?# while_loop
while ?Comp(?i, ?n):
    ?
    ?Incr(?i, 1)
